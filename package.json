{
  "name": "some-tsconfig-preset",
  "version": "3.0.0",
  "keywords": [
    "tsconfig preset",
    "strict tsconfig preset",
    "tsconfig base"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DaniFoldi/some-tsconfig-preset"
  },
  "license": "MIT",
  "author": "DaniFoldi",
  "type": "module",
  "main": "base.json",
  "bin": {
    "some-tsconfig-preset": "./dist/cli.js"
  },
  "files": [
    "base.json",
    "node.json",
    "react.json",
    "react-native.json",
    "scripts.json",
    "workers.json",
    "workers-vitest.json",
    "presets",
    "dist"
  ],
  "scripts": {
    "build": "esbuild scripts/init.ts --bundle --platform=node --format=esm --outdir=dist --entry-names=cli --packages=external --banner:js='#!/usr/bin/env node' --log-level=info",
    "lint": "eslint --cache .",
    "prepublishOnly": "pnpm run build",
    "release": "changeset publish",
    "typecheck": "tsc",
    "version-packages": "changeset version"
  },
  "dependencies": {
    "detect-package-manager": "^3.0.2",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "semver": "^7.7.3"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.2",
    "@changesets/cli": "2.29.8",
    "@types/node": "24.10.4",
    "@types/prompts": "2.4.9",
    "@types/semver": "7.7.1",
    "bundled-eslint-config": "0.7.3",
    "esbuild": "0.27.2",
    "eslint": "9.39.2",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.27.0"
}
