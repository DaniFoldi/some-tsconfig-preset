{
  "name": "some-tsconfig-preset",
  "version": "2.0.7",
  "main": "tsconfig.json",
  "type": "module",
  "scripts": {
    "build": "esbuild scripts/init.ts --bundle --platform=node --format=esm --outdir=dist --entry-names=cli --packages=external --banner:js='#!/usr/bin/env node' --log-level=info",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint --cache .",
    "typecheck": "tsc -b --noEmit"
  },
  "keywords": [
    "tsconfig preset",
    "strict tsconfig preset",
    "tsconfig base"
  ],
  "files": [
    "base.json",
    "node.json",
    "react.json",
    "react-native.json",
    "workers.json",
    "workers-vitest.json",
    "presets",
    "dist",
    "global.json"
  ],
  "bin": {
    "some-tsconfig-preset": "./dist/cli.js"
  },
  "author": "DaniFoldi",
  "license": "MIT",
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "detect-package-manager": "^2.0.1",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "bundled-eslint-config": "^0.6.7",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.24.0"
}
